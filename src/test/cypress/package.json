{
    "name": "artemis_cypress",
    "description": "Cypress tests for Artemis",
    "private": true,
    "license": "MIT",
    "cacheDirectories": [
        "node_modules"
    ],
    "devDependencies": {
        "@4tw/cypress-drag-drop": "2.2.3",
        "@heddendorp/coverage-git-compare": "1.9.9",
        "@heddendorp/cypress-plugin-multilanguage-coverage": "1.7.6",
        "@types/node": "18.15.11",
        "cypress": "12.9.0",
        "cypress-file-upload": "5.0.8",
        "cypress-wait-until": "1.7.2",
        "typescript": "4.9.5",
        "uuid": "9.0.0",
        "wait-on": "7.0.1"
    },
    "scripts": {
        "cypress:open": "cypress open --browser=edge",
        "cypress:opencoder": "DISPLAY=:90 cypress open --browser=chrome",
        "cypress:run": "cypress run --browser=chrome",
        "cypress:record": "set CYPRESS_API_URL='http://192.168.3.144:1234/' && npx cy2 run --browser=edge --record --key somekey",
        "detect:flakies": "coverage-git-compare bamboo $BAMBOO_PLAN_KEY $BAMBOO_BUILD_NUMBER $BAMBOO_TOKEN",
        "update": "npm-upgrade"
    }
}
